<global_conf>
	<request_list>
		<request>
			<name>Login</name>
			<url>http://www.kaixin001.com/login/login.php</url>
			<params>
				<url>/home/</url>
				<email input='1'></email>
				<password input='1'></password>
			</params>
			<method>POST</method>
		</request>
		<request>
			<name>Logout</name>
			<url>http://www.kaixin001.com/login/logout.php</url>
			<params></params>
			<method>GET</method>
		</request>
		<request>
			<name>FriendsList</name>
			<url>http://www.kaixin001.com/interface/suggestfriend.php</url>
			<params>
				<type>all</type>
			</params>
			<method>GET</method>
		</request>
		<request>
			<name>Verify</name>
			<url>http://www.kaixin001.com/home/</url>
			<params></params>
			<method>GET</method>
		</request>
		<request>
			<name>CropInfo</name>
			<url>http://www.kaixin001.com/house/garden/getconf.php</url>
			<params>
				<verify input='1'></verify>
				<fuid input='1'></fuid>
				<r>1</r>
			</params>
			<method>GET</method>
		</request>
		<request>
			<name>Plough</name>
			<url>http://www.kaixin001.com/house/garden/plough.php</url>
			<params>
				<verify input='1'></verify>
				<seedid>0</seedid>
				<fuid input='1'></fuid>
				<farmnum input='1'></farmnum>
				<r>1</r>
			</params>
			<method>GET</method>
		</request>
		<request>
			<name>SeedList</name>
			<url>http://www.kaixin001.com/house/garden/myseedlist.php</url>
			<params>
				<verify input='1'></verify>
				<page input='1'></page>
				<r>1</r>
			</params>
			<method>GET</method>
		</request>
        <request>
            <name>FarmSeed</name>
            <url>http://www.kaixin001.com/house/garden/farmseed.php</url>
            <params>
				<verify input='1'></verify>
                <seedid input='1'></seedid>
                <fuid input='1'></fuid>
                <farmnum input='1'></farmnum>
                <r>1</r>
            </params>
            <method>POST</method>
        </request>
		<request>
			<name>Havest</name>
			<url>http://www.kaixin001.com/house/garden/havest.php</url>
			<params>
				<verify input='1'></verify>
				<fuid input='1'></fuid>
				<farmnum input='1'></farmnum>
				<r>1</r>
			</params>
		</request>
	</request_list>
	<attrib_list>
		<basic>
			<item>farmnum</item>
			<item>water</item>
			<item>vermin</item>
			<item>cropsid</item>
			<item>fuid</item>
			<item>status</item>
			<item>grass</item>
			<item>shared</item>
		</basic>
		<extent>
			<item>cropsstatus</item>
			<item>grow</item>
			<item>totalgrow</item>
			<item>seedid</item>
			<item>crops</item>
			<item>farm</item>
		</extent>
	</attrib_list>
</global_conf>
